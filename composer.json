    },
    "autoload": {
        "psr-4": {
            "LoganSong\\LaravelMultiDatabase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LoganSong\\LaravelMultiDatabase\\ServiceProvider"

{
    "name": "song/laravel-multi-database",
    "description": "laravel multi databasupporter",
    "keywords": [
        "database",
        "multi"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "logan-song",
            "email": "jongno@kakao.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
